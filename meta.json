{
  "module_id": "viam:obstacles_2d_lidar",
  "visibility": "public",
  "url": "https://github.com/viamrobotics/obstacles-2d-lidar",
  "description": "This vision service uses planar lidar as input to detect obstacles. It implements the method get_object_point_clouds() of the vision service API.",
  "models": [
    {
      "api": "rdk:service:vision",
      "model": "viam:vision:obstacles_2d_lidar"
    }
  ],
  "build": {
    "build": "./build.sh",
    "path": "dist/archive.tar.gz",
    "arch": [
      "linux/amd64",
      "linux/arm64",
      "darwin/arm64"
    ]
  },
  "entrypoint": "dist/main"
}